{
  "name": "frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:de": "ng serve --configuration=de",
    "build": "ng build",
    "build:de": "ng build --configuration=de",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "test:coverage": "ng test --code-coverage",
    "test:coverage-watch": "ng test --code-coverage --watch",
    "test:ci": "ng test --watch=false --browsers=ChromeHeadless --code-coverage",
    "lint": "ng lint",
    "format": "prettier --write \"src/**/*.{ts,html,css,scss,json}\"",
    "format:check": "prettier --check \"src/**/*.{ts,html,css,scss,json}\"",
    "lint:fix": "ng lint --fix",
    "generate:swagger": "ng-openapi-gen -i http://localhost:3000/api-docs/swagger.json -o src/app/shared/api"
  },
  "private": true,
  "dependencies": {
    "@angular-eslint/eslint-plugin": "^21.0.1",
    "@angular/cdk": "^21.0.0",
    "@angular/common": "^21.0.1",
    "@angular/compiler": "^21.0.1",
    "@angular/core": "^21.0.1",
    "@angular/forms": "^21.0.1",
    "@angular/material": "^21.0.0",
    "@angular/platform-browser": "^21.0.1",
    "@angular/router": "^21.0.1",
    "rxjs": "~7.8.2",
    "tslib": "^2.8.1",
    "zone.js": "^0.16.0"
  },
  "devDependencies": {
    "@angular/build": "^21.0.0",
    "@angular/cli": "^21.0.0",
    "@angular/compiler-cli": "^21.0.1",
    "@angular/localize": "^21.0.1",
    "@eslint/js": "^9.39.1",
    "@types/jasmine": "~5.1.13",
    "angular-eslint": "21.0.1",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "globals": "^16.5.0",
    "jasmine-core": "~5.12.1",
    "karma": "~6.4.4",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.1",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "prettier": "^3.7.3",
    "typescript": "~5.9.3",
    "typescript-eslint": "8.48.0"
  }
}
